@using ShwasherSys.Views.Shared.New.Query
@model string
@{
    string originFiled = "id,userName,name,emailAddress",
        //targetFiled = string.IsNullOrEmpty(Model) ? originFiled : Model;
        targetFiled = Model ?? "";
    List<QueryItem> queryItems = new List<QueryItem>()
    {
        new QueryItem("userName",L("userName"),true),
        new QueryItem("name",L("realName"),true),
        new QueryItem("emailAddress",L("emailAddress"),true),

    };
    
    QueryViewModel qmModel = new QueryViewModel(L("UserInfo"), "/api/services/app/Query/GetUsers", queryItems, originFiled, targetFiled, modalId: "query_user");
}
@Html.Partial("Query/_QueryModal", qmModel)